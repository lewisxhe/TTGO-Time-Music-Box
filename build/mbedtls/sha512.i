# 539 "D:/ESP32/esp-idf-v3.1-rc1/components/mbedtls/mbedtls/library/sha512.c" 3 4
       ((void *)0) 
# 539 "D:/ESP32/esp-idf-v3.1-rc1/components/mbedtls/mbedtls/library/sha512.c"
            == buf )
    {
        if( verbose != 0 )
            printf( "Buffer allocation failed\n" );

        return( 1 );
    }

    mbedtls_sha512_init( &ctx );

    for( i = 0; i < 6; i++ )
    {
        j = i % 3;
        k = i < 3;

        if( verbose != 0 )
            printf( "  SHA-%d test #%d: ", 512 - k * 128, j + 1 );

        if( ( ret = mbedtls_sha512_starts_ret( &ctx, k ) ) != 0 )
            goto fail;

        if( j == 2 )
        {
            memset( buf, 'a', buflen = 1000 );

            for( j = 0; j < 1000; j++ )
            {
                ret = mbedtls_sha512_update_ret( &ctx, buf, buflen );
