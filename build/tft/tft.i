color_t HSBtoRGB(float _hue, float _sat, float _brightness)
{
 float red = 0.0;
 float green = 0.0;
 float blue = 0.0;

 if (_sat == 0.0)
 {
  red = _brightness;
  green = _brightness;
  blue = _brightness;
 }
 else
 {
  if (_hue == 360.0)
  {
   _hue = 0;
  }

  int slice = (int)(_hue / 60.0);
  float hue_frac = (_hue / 60.0) - slice;

  float aa = _brightness * (1.0 - _sat);
  float bb = _brightness * (1.0 - _sat * hue_frac);
  float cc = _brightness * (1.0 - _sat * (1.0 - hue_frac));

  switch (slice)
  {
  case 0:
   red = _brightness;
   green = cc;
   blue = aa;
   break;
  case 1:
   red = bb;
   green = _brightness;
   blue = aa;
   break;
  case 2:
   red = aa;
   green = _brightness;
   blue = cc;
   break;
  case 3:
   red = aa;
   green = bb;
   blue = _brightness;
   break;
  case 4:
   red = cc;
   green = aa;
   blue = _brightness;
   break;
  case 5:
   red = _brightness;
   green = aa;
   blue = bb;
   break;
  default:
   red = 0.0;
   green = 0.0;
   blue = 0.0;
   break;
  }
 }

 color_t color;
 color.r = ((uint8_t)(red * 255.0)) & 0xFC;
 color.g = ((uint8_t)(green * 255.0)) & 0xFC;
 color.b = ((uint8_t)(blue * 255.0)) & 0xFC;

 return color;
}

